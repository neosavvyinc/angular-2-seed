{
  "name": "ns-ng2-seed",
  "version": "1.0.0",
  "description": "neosavvy's homegrown ng2 seed",
  "engines": {
    "npm": "3.10.3",
    "node": "6.3.0"
  },
  "scripts": {
    "build": "npm run lint && npm run test:build && npm run build:dist",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config config/webpack/webpack.config.js --bail",
    "lint": "tslint ./src/**/*.ts",
    "postinstall": "typings install",
    "start": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.js --inline --progress",
    "test": "npm run lint && npm run test:dev",
    "test:build": "NODE_ENV=test karma start config/karma/karma.conf.js --single-run",
    "test:dev": "NODE_ENV=test karma start config/karma/karma.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "0.4.0",
    "autoprefixer": "6.3.7",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "2.0.0-beta.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs2-launcher": "0.5.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "3.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "tslint": "3.13.0",
    "tslint-eslint-rules": "1.3.0",
    "tslint-loader": "2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "2.1.0-beta.17",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
